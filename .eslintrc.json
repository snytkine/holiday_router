{
  "extends": ["airbnb/base"],
  "plugins": ["@typescript-eslint"],
  "parser": "@typescript-eslint/parser",
  "rules": {
    "import/no-unresolved": 0,
    "import/extensions": 0,
    "no-unused-vars": "off",
    "no-restricted-syntax": "off",
    "import/prefer-default-export": "warn",
    "@typescript-eslint/no-unused-vars": "warn"
  }
}
