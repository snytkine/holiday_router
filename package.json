{
  "name": "thanksgiving-router",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "prebuild": "rm -rf build",
    "build": "tsc --pretty",
    "test": "nyc mocha build/tests/ --recursive"
  },
  "nyc": {
    "extension": [
      ".ts",
      ".tsx"
    ],
    "exclude": [
      "**/*.d.ts",
      "**/sandbox/**",
      "**/interfaces/**",
      "**/coverage/**",
      "**/tests/**",
      "**/index.ts"
    ],
    "reporter": [
      "html"
    ],
    "all": true
  },
  "dependencies": {
    "constantinople": "^3.1.1",
    "debug": "^4.1.1",
    "lodash": "4.17.12",
    "source-map-support": "^0.5.16",
    "uglify-js": "^2.6.0"
  },
  "devDependencies": {
    "@types/chai": "^4.1.7",
    "@types/debug": "^4.1.5",
    "@types/mocha": "^5.2.6",
    "@types/node": "^12.12.14",
    "chai": "^4.2.0",
    "chai-arrays": "^2.0.0",
    "mocha": "^6.2.2",
    "nyc": "^14.1.1",
    "typescript": "^3.7.2"
  }
}
